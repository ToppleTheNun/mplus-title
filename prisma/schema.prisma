generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model History {
    id Int @id @default(autoincrement())

    timestamp Int
    faction   Factions
    region    Regions

    rioScore Float
    rioRank  Int

    customScore Float
    customRank  Int
}

enum Factions {
    horde
    alliance
}

enum Regions {
    eu
    kr
    tw
    us
}
