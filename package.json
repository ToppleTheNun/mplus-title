{
  "name": "mplus-title",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"yarn dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "postinstall": "yarn-deduplicate -l && sort-package-json && rm .eslintrc.json && node node_modules/eslint-config-galex/dist/generateStandalone",
    "install:clean": "rm -rf node_modules && rm yarn.lock && yarn",
    "lint": "eslint",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "remix:dev": "remix dev",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "4.5.0",
    "@remix-run/node": "1.7.4",
    "@remix-run/react": "1.7.4",
    "@remix-run/vercel": "1.7.4",
    "@vercel/node": "2.5.26",
    "cheerio": "1.0.0-rc.10",
    "highcharts": "10.2.1",
    "highcharts-react-official": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.7.4",
    "@remix-run/serve": "1.7.4",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/tailwindcss": "3.1.0",
    "autoprefixer": "10.4.13",
    "concurrently": "7.5.0",
    "eslint": "8.26.0",
    "eslint-config-galex": "4.3.1",
    "postcss": "8.4.18",
    "prisma": "4.5.0",
    "sort-package-json": "2.0.0",
    "typescript": "4.8.4",
    "yarn-deduplicate": "6.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
