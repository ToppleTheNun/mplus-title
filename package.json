{
  "name": "mplus-title",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"yarn dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "postinstall": "yarn-deduplicate -l && sort-package-json && rm .eslintrc.json && node node_modules/eslint-config-galex/dist/generateStandalone",
    "install:clean": "rm -rf node_modules && rm yarn.lock && yarn",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "remix:dev": "remix dev"
  },
  "dependencies": {
    "@prisma/client": "4.0.0",
    "@remix-run/node": "1.6.2",
    "@remix-run/react": "1.6.2",
    "@remix-run/vercel": "1.6.2",
    "@vercel/node": "2.3.1",
    "cheerio": "1.0.0-rc.10",
    "highcharts": "10.1.0",
    "highcharts-react-official": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.1.4"
  },
  "devDependencies": {
    "@remix-run/dev": "1.6.2",
    "@remix-run/serve": "1.6.2",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/tailwindcss": "3.0.11",
    "autoprefixer": "10.4.7",
    "concurrently": "7.2.2",
    "eslint": "8.18.0",
    "eslint-config-galex": "4.1.4",
    "postcss": "8.4.14",
    "prisma": "4.0.0",
    "sort-package-json": "1.57.0",
    "typescript": "4.7.4",
    "yarn-deduplicate": "5.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
